  "description": "Etherith - React + Tailwind CSS Memory Vault with Discord OAuth, IPFS Storage, and AI Moderation",
  "type": "module",
  "main": "src/main.jsx",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run post-build && wrangler pages deploy dist --project-name=etherith-production",
    "build-only": "vite build && npm run post-build",
    "post-build": "copy dist\\index-react.html dist\\index.html",
