{
  "name": "yjs-pwa-demo",
  "version": "2.0.0",
  "description": "Yjs Collaborative PWA Demo - Real-time collaboration with Yjs CRDT",
  "type": "module",
  "main": "index.html",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler pages deploy dist --project-name=etherith-mvp",
    "deploy-git": "git add . && git commit -m 'Deploy updates' && git push origin main",
    "cf-dev": "wrangler pages dev dist",
    "cf-preview": "wrangler pages dev dist --compatibility-date=2025-09-21",
    "setup-discord": "node scripts/setup-discord.js"
  },
  "keywords": [
    "yjs",
    "collaborative",
    "crdt",
    "pwa",
    "progressive-web-app",
    "quill",
    "real-time",
    "cloudflare",
    "pages"
  ],
  "author": "Claude Code",
  "license": "MIT",
  "dependencies": {
    "yjs": "^13.6.27",
    "y-quill": "^0.1.5",
    "y-websocket": "^1.5.0",
    "y-protocols": "^1.0.6",
    "quill": "^1.3.7"
  },
  "devDependencies": {
    "vite": "^5.4.0",
    "vite-plugin-pwa": "^0.21.2",
    "wrangler": "^4.38.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
